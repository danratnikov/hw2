/*Fonts css*/

@css {
    @font-face {
        font-family: 'elektra_light_probold';
        src: url('../font/elektra-light-pro-bold-webfont.woff') format('woff'),
             url('../font/elektra-light-pro-bold-webfont.ttf') format('truetype'),
             url('../font/elektra-light-pro-bold-webfont.svg#elektra_light_probold') format('svg');
        font-weight: normal;
        font-style: normal;
    
    }
    
    @font-face {
        font-family: 'elektra_light_proregular';
        src: url('../font/elektra-light-pro-webfont.woff') format('woff'),
             url('../font/elektra-light-pro-webfont.ttf') format('truetype'),
             url('../font/elektra-light-pro-webfont.svg#elektra_light_proregular') format('svg');
        font-weight: normal;
        font-style: normal;
    
    }
    
    @font-face {
        font-family: 'elektra_medium_proitalic';
        src: url('../font/elektra-medium-pro-italic-webfont.woff') format('woff'),
             url('../font/elektra-medium-pro-italic-webfont.ttf') format('truetype'),
             url('../font/elektra-medium-pro-italic-webfont.svg#elektra_medium_proitalic') format('svg');
        font-weight: normal;
        font-style: normal;
    
    }

    @font-face {
        font-family: 'elektra_medium_proregular';
        src: url('../font/elektra-medium-pro-webfont.woff') format('woff'),
             url('../font/elektra-medium-pro-webfont.ttf') format('truetype'),
             url('../font/elektra-medium-pro-webfont.svg#elektra_medium_proregular') format('svg');
        font-weight: normal;
        font-style: normal;
    
    }
    
    // Danil, please add more @font-face here
}